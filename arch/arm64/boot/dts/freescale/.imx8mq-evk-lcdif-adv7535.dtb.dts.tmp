    compatible = "fsl,imx8mq-ccm";
    reg = <0x30380000 0x10000>;
    interrupts = <0 85 4>,
                 <0 86 4>;
    #clock-cells = <1>;
    clocks = <&ckil>, <&osc_25m>, <&osc_27m>,
             <&clk_ext1>, <&clk_ext2>,
             <&clk_ext3>, <&clk_ext4>;
    clock-names = "ckil", "osc_25m", "osc_27m",
                  "clk_ext1", "clk_ext2",
                  "clk_ext3", "clk_ext4";
    assigned-clocks = <&clk 113>,
        <&clk 105>,
        <&clk 117>,
        <&clk 25>,
        <&clk 30>;
    assigned-clock-parents = <&clk 78>,
        <&clk 76>,
        <&clk 86>;
    assigned-clock-rates = <800000000>,

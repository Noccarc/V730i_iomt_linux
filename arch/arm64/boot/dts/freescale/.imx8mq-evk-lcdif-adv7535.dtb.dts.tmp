  };

  bus@30000000 {
   compatible = "fsl,imx8mq-aips-bus", "simple-bus";
   #address-cells = <1>;
   #size-cells = <1>;
   ranges = <0x30000000 0x30000000 0x400000>;

   sai1: sai@30010000 {
    compatible = "fsl,imx8mq-sai",
          "fsl,imx6sx-sai";
    reg = <0x30010000 0x10000>;
    interrupts = <0 95 4>;
    clocks = <&clk 238>,
     <&clk 0>,
     <&clk 196>,
     <&clk 0>, <&clk 0>;
    clock-names = "bus", "mclk0", "mclk1", "mclk2", "mclk3";
    dmas = <&sdma2 8 1 0>, <&sdma2 9 1 0>;

    dmas = <&sdma2 4 24 0>, <&sdma2 5 24 0>;
    dma-names = "rx", "tx";
    fsl,shared-interrupt;
    status = "disabled";
   };

   sai5: sai@30040000 {
    compatible = "fsl,imx8mq-sai",
          "fsl,imx6sx-sai";
    reg = <0x30040000 0x10000>;
    interrupts = <0 90 4>;
    clocks = <&clk 242>,
     <&clk 0>,
     <&clk 200>,
     <&clk 0>, <&clk 0>;
    clock-names = "bus", "mclk0", "mclk1", "mclk2", "mclk3";
    dmas = <&sdma2 2 24 0>, <&sdma2 3 24 0>;
    dma-names = "rx", "tx";
    fsl,shared-interrupt;

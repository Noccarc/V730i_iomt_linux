 sec_mu4: mu@31580000 {
  compatible = "fsl,imx8-mu-seco";
  reg = <0x31580000 0x10000>;
  interrupts = <0 450 4>;
  #mbox-cells = <2>;
  power-domains = <&pd 505>;
  status = "okay";
 };
};

seco_mu1: seco_mu1 {
 compatible = "fsl,imx-seco-mu";
 mbox-names = "txdb", "rxdb";
